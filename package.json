{
  "name": "image-search",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node ./dist/server.js",
    "dev": "concurrently \"npm run nodemon\" \"npm run build.w\"",
    "prebuild": "npm run clean",
    "build": "concurrently \"npm run build.js\" \"npm run build.html\"",
    "build.js": "tsc",
    "build.html": "copyfiles -u 1 ./src/**/*.html ./dist",
    "build.w": "tsc -w",
    "nodemon": "set NODE_ENV=development && nodemon --watch dist dist/server.js",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonnyFox/image-search.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JonnyFox/image-search/issues"
  },
  "homepage": "https://github.com/JonnyFox/image-search#readme",
  "dependencies": {
    "@types/express": "^4.0.33",
    "@types/mongodb": "^2.1.33",
    "@types/node": "^6.0.46",
    "bing.search": "^1.0.1",
    "concurrently": "^3.1.0",
    "copyfiles": "^1.0.0",
    "express": "^4.14.0",
    "inversify": "^3.0.0-beta.1",
    "mongodb": "^2.2.11",
    "node-bing-api": "^3.0.1",
    "reflect-metadata": "^0.1.8",
    "rimraf": "^2.5.4",
    "typescript": "2.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
